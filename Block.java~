import java.awt.*;

public class Block{

    public Block(int x, int y, ID id){
	super(x,y,id);
    }

    public void tick(){

    }

    public void render(Graphics g){
	g.setColor(color.black);
	g.fillRect(x,y,32,32);
    }

    public Rectangle getBounds(){
	return new Rectangle(x,y,32,32);
    }
    
}
